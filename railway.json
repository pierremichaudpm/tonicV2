{
  "$schema": "https://railway.app/railway.schema.v2.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm install --omit=dev && npm run build"
  },
  "deploy": {
    "startCommand": "npm start",
    "numReplicas": 1,
    "healthcheckPath": "/healthz",
    "healthcheckTimeout": 10
  }
}

